# Reload functions
super + Escape
	slock

super + shift + alt + k
    killall -SIGUSR1 sxhkd

super + alt + d
    dmenu-display-config

super + alt + p
    j4-dmenu-desktop --dmenu="dmenu"

super + alt + v
	dmenu-greenclip

super + alt + r
	dmenu-remmina

super + alt + n
    dmenu-networkmanager

super + alt + s
    dmenu-audio-select

super + alt + l
    dmenu-lastpass copy

super + ctrl + b
    qutebrowser

super + ctrl + e
    evolution

super + ctrl + f
    kitty -e ranger

XF86AudioRaiseVolume
	volume up

XF86AudioLowerVolume
	volume down

XF86AudioMute
    volume mute
